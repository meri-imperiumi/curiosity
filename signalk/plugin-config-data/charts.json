{
  "enabled": true,
  "enableLogging": false,
  "configuration": {
    "onlineChartProviders": [
      {
        "name": "OSM",
        "minzoom": 1,
        "maxzoom": 18,
        "format": "png",
        "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"
      },
      {
        "name": "OSM dark",
        "minzoom": 1,
        "maxzoom": 18,
        "format": "png",
        "url": "https://cartodb-basemaps-a.global.ssl.fastly.net/dark_all/{z}/{x}/{y}.png"
      },
      {
        "name": "OpenSeaMap",
        "minzoom": 1,
        "maxzoom": 18,
        "format": "png",
        "url": "http://t1.openseamap.org/seamark/{z}/{x}/{y}.png"
      }
    ]
  }
}